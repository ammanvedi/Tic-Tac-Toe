<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.comp1008.AmmanVedi.TicTacToe"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.comp1008.AmmanVedi.TicTacToe.TicTacToeActivity"
            android:label="@string/app_name"
            android:noHistory="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.comp1008.AmmanVedi.TicTacToe.SplashScreen"
            android:label="@string/title_activity_splash_screen" >
        </activity>
        <activity
            android:name="com.comp1008.AmmanVedi.TicTacToe.DifficultySelect"
            android:label="@string/title_activity_difficulty_select" >
        </activity>
        <activity
            android:name="com.comp1008.AmmanVedi.TicTacToe.HighScores"
            android:label="@string/title_activity_high_scores" >
        </activity>
    </application>

</manifest>